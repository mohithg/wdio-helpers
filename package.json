{
  "name": "wdio-helpers",
  "version": "1.0.2",
  "description": "Webdriver IO helper functions",
  "main": "build/helpers.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf build && mkdir build && node_modules/.bin/babel src/helpers.js > build/helpers.js",
    "generate_doc": "node_modules/.bin/jsdoc -c jsdoc.conf.json * -t node_modules/docdash",
    "generate_api_doc": "jsdoc2md src/helpers.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohithg/wdio-helpers.git"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "webdriverio": "^4.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "docdash": "^0.4.0",
    "jsdoc": "^3.4.3"
  },
  "keywords": [
    "wdio",
    "helpers",
    "automation",
    "webdriver",
    "selenium"
  ],
  "author": "Mohith G",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohithg/wdio-helpers/issues"
  },
  "homepage": "https://github.com/mohithg/wdio-helpers#readme"
}
